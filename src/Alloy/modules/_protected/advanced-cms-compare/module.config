<?xml version="1.0" encoding="utf-8"?>
<module clientResourceRelativePath="1.0.0">

  <assemblies>
    <add assembly="AdvancedCMS.Compare" />
  </assemblies>

  <clientResources>
    <add name="epi-cms.widgets.base" path="~/modules/customWidgetSwitcher.js" resourceType="Script" />
  </clientResources>

  <clientModule initializer="advanced-cms-compare.main-initializer">
    <moduleDependencies>
      <add dependency="CMS" type="RunAfter" />
    </moduleDependencies>
    <requiredResources>
      <add name="AdvancedCMS.Compare" />
    </requiredResources>
  </clientModule>

  <dojo>
    <paths>
      <add name="advanced-cms-compare" path="Scripts" />
    </paths>
 </dojo>
</module>
